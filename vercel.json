{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/profiles.js": {
      "includeFiles": "data/**/*"
    },
    "api/profiles/*.js": {
      "includeFiles": "data/**/*"
    },
    "api/**/*.js": {
      "includeFiles": "data/**/*"
    }
  }
}